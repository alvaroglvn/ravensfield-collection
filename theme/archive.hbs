{{!< default}}

<div class="gh-page">
    <div class="gh-container">
        {{#if @member}}
        <main class="latest-posts">
            {{#get "posts" limit="all" order="published_at desc"}}
        {{#foreach posts visibility="all"}}
        <div class="post-container">
            {{> card-sm }}
        </div>
        {{/foreach}}
        {{/get}}
        </main>
        {{/if}}
    </div>
</div>
